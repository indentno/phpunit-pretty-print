{
    "name": "sempro/phpunit-pretty-print",
    "version": "1.4.0",
    "description": "Prettify PHPUnit output",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.1.0",
        "phpunit/phpunit": "^7 || ^8 || ^9 || ^10"
    },
    "autoload": {
        "psr-4": {
            "Sempro\\PHPUnitPrettyPrinter\\": "src/"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16"
    }
}
